{
  "name": "niplayer",
  "version": "1.4.6",
  "description": "This is a TS library for video player",
  "main": "./dist/player.esm.js",
  "module": "./dist/player.esm.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "rspack serve --config ./config/rspack.dev.config.cjs"
  },
  "exports": {
    ".": {
      "require": "./dist/player.cjs.js",
      "import": "./dist/player.esm.js"
    }
  },
  "bugs": {
    "url": "https://github.com/NiMediaOrg/NiPlayer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NiMediaOrg/NiPlayer"
  },
  "keywords": [
    "player",
    "typescript",
    "library",
    "danmaku",
    "stream media protocol",
    "dash",
    "hls",
    "flv",
    "mp4",
    "bilibili mode"
  ],
  "author": "NovaNi",
  "license": "MIT",
  "browserslist": [
    "defaults",
    "not ie < 8",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "dependencies": {
    "be-full": "^0.1.4",
    "eventemitter3": "^5.0.1",
    "mobx": "^6.13.1",
    "mp4box": "^0.5.2",
    "niplayer-components": "workspace:^1.0.0",
    "uuid": "^10.0.0"
  },
  "files": [
    "package.json",
    "./dist/*"
  ]
}